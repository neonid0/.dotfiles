{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "kitty",
        "source": "$(~/.config/fastfetch/get_music_artwork.sh)",
        "width": 24,
        "height": 12,
        "padding": {
            "left": 10,
            "top": 1
        }
    },
    "modules": [
        "break",
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌─────────────────────────────Now Playing──────────────────────────────┐"
        },
        {
            "type": "media",
            "key": "  Title",
            "keyColor": "cyan"
        },
        {
            "type": "media",
            "key": "  Artist",
            "format": "{artist}",
            "keyColor": "cyan"
        },
        {
            "type": "media",
            "key": "  Album",
            "format": "{album}",
            "keyColor": "cyan"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└──────────────────────────────────────────────────────────────────────┘"
        },
        "break",
        {
            "type": "uptime",
            "key": "  Uptime",
            "keyColor": "magenta"
        },
        "break"
    ]
}
